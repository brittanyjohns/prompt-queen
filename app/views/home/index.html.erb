<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<%= form_for :site_parser, url: "/site_parser", method: :post do |form| %>
  <div class="col-md-12">
    <%= form.file_field :doc, class: "form-control inline", placeholder: "link" %>
    <%= form.submit "Go", class: "btn btn-success" %>
  </div>
<% end %>

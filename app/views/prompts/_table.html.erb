<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Body</th>
    </tr>
  </thead>
  <tbody>
    <% prompt.responses.each do |response| %>
      <tr>
        <td><%= response.id %></td>
        <td><%= link_to response.prompt_text, response %></td>
        <td><%= response.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
